.containers{
    margin-top: 10vh;
    padding: 0 30rpx;
}

.nav-bar-title-style{
    font-size: 40rpx!important;
    font-weight: bold!important;
}

.top-setting-group{
    display: flex;
    justify-content: space-evenly;
    align-items: center;
    width: 20vw;
    height: 5vh;
}

.top-setting-icon{
    height: 50rpx;
    width: 50rpx;
    top: 5vh;
}

.top-share-icon{
    height: 50rpx;
    width: 50rpx;
    top: 5vh;
}   

.imagesize{
 display:flex;             
 justify-content: center;  
 padding: 10rpx;      
}
.imagesize image { 
  width:600rpx;
  height:420rpx;
}
.imagesizemid{
    display:flex;             
    justify-content: center;  
    padding: 4px;
}
