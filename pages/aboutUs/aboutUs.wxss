     width:750rpx;
     height:1000rpx;
}
.container{
    display:flex;             
    justify-content: center;
    align-items: center;
    height: 80rpx;
}
.text_mid_size{
    font-size: large;
    color: #444;
}
.slides{}
.slides swiper{
    height: 420rpx;
}
.slides  navigator{
    height: 100%;
    width: 100%;
}
.slides image{
    height: 100%;
    width: 100%;
}
.zero{
    color: rgb(255, 255, 255);
}
.text_last_size{
    font-size: small;
    color: #444;
}
