
.container{
    margin: 12vh 30rpx 50rpx;
}

.go-page-top{
    height: 80rpx;
    width: 80rpx;
    position: fixed;
    bottom: 5vh;
    right: 8vw;
}

.top-setting-group{
    display: flex;
    justify-content: space-evenly;
    align-items: center;
    width: 20vw;
    height: 5vh;
}

.top-setting-icon{
    height: 50rpx;
    width: 50rpx;
    top: 5vh;
}

.top-share-icon{
    height: 50rpx;
    width: 50rpx;
    top: 5vh;
}

.help-item-group{
    margin-bottom: 50rpx;
}

.help-info{
    margin: 20rpx 0;
    text-indent: 2em;
    font-size: 30rpx;
    color: gray;
}

.help-title{
    font-size: x-large;
    font-weight: bolder;
    margin: 20rpx;
}

.link-top {
    width: 100%;
    height: 1px;
    border-top: solid rgb(220, 221, 223) 1rpx;
}

.img-container{
    display:flex;
    justify-content: center;
}

.help-img{
    margin: 20 rpx;
    border:greensolid1px;
    width: 90vw;
}